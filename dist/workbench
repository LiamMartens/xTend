#!/usr/bin/env php
<?php
    namespace xTend\Application;
    //require files
    require_once("System/Core/App.php");
    require_once("System/Workbench/Workbench.php");
    //use workbench
    use \xTend\Workbench\Workbench as Workbench;
    //create app
    $app=\xTend\Core\createNewApp(__NAMESPACE__, __DIR__, true);
    $app->getFileHandler()->system("Config.App.php")->include();
    //start workbench
    $bench = new Workbench($app);
    //run app
    $app->run();
